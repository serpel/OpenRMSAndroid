<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.intellisysla.rmsscanner">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UI.View.Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UI.View.Activity.LocationActivity" />
        <activity android:name=".UI.View.Activity.SearchActivity" />
        <activity android:name=".UI.View.Activity.ItemListActivity" />
        <activity
            android:name=".UI.View.Activity.SearchListActivity"
            android:label="@string/title_activity_search_list"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>